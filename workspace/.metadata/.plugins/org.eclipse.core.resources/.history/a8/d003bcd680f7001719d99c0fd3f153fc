package sgbd.impl;

import sgbd.stockage.*;

public class JointureBl {

	/*
	 * att1 et att2 sont les numÃ©ros de colonne.
	 */
	public Nuplet[] joinBl(Nuplet[] t1, int att1, Nuplet[] t2, int att2){
		NupletInt[] result;
		if(t1.length>t2.length)
			result = new NupletInt[t1.length];
		else
			result = new NupletInt[t2.length];
		
		return null;
	}
}
