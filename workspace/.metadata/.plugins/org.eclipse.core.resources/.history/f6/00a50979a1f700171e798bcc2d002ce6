package sgbd.impl;


import sgbd.stockage.*;
import sgbd.impl.*;
import java.util.regex.Matcher;
import java.util.regex.Pattern;

public class Parser {
	
	
	public String[] Parser(String commande)
	{
		Pattern selectfrom = Pattern.compile("(?i)\\s*select(?:((?:\\w+\\s*,\\s*)*)\\w+|[*]) from (\\w+)\\s*;");
		Pattern selectfromwhere = Pattern.compile("(?i)\\s*select(?:((?:\\w+\\s*,\\s*)*)\\w+|[*]) from (\\w+)\\s* where (\\w+)=|<|>(\\w+);");
				
		Matcher selectM = selectfrom.matcher(commande);
		return null;
	}
}
